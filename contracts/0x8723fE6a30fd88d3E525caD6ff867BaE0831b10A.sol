contract main {




// =====================  Runtime code  =====================


uint256 someValue;

function someValue() payable {
    return someValue
}

function _fallback() payable {
    revert
}

function Addition(uint256 arg1, uint256 arg2) payable {
    require calldata.size - 4 >= 64
    require arg1 == arg1
    require arg2 == arg2
    if arg1 > -arg2 - 1:
        revert with 'NH{q', 17
    return (arg1 + arg2)
}



}
