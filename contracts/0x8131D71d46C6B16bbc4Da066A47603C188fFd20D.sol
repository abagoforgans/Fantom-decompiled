        revert with 0, 'Already minter'
    stor6[address(arg1)] = 1
    stor7.length++
    stor7[stor7.length].field_0 = arg1
    emit MinterAdded(arg1);
}

function decreaseMaxSupply(uint256 arg1) payable {
    require calldata.size - 4 >= 32
