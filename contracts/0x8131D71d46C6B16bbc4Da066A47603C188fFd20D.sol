    if not stor6[address(arg1)]:
        revert with 0, 'Not minter'
    stor6[address(arg1)] = 0
    idx = 0
    while idx < stor7.length:
        mem[0] = 7
        if stor7[idx].field_0 == arg1:
            if stor7.length < 1:
                revert with 'NH{q', 17
            if stor7.length - 1 >= stor7.length:
                revert with 'NH{q', 50
            if idx >= stor7.length:
                revert with 'NH{q', 50
            stor7[idx].field_0 = stor7[stor7.length].field_0
            if not stor7.length:
                revert with 'NH{q', 49
            mem[0] = 7
            stor7[stor7.length].field_0 = 0
            stor7.length--
        if idx == -1:
            revert with 'NH{q', 17
        idx = idx + 1
        continue 
    emit MinterRemoved(arg1);
}

function getMinters() payable {
    mem[64] = (32 * stor7.length) + 128
    mem[96] = stor7.length
    if not stor7.length:
        mem[(32 * stor7.length) + 128] = 32
        mem[(32 * stor7.length) + 160] = stor7.length
        idx = 0
        s = 128
        t = (32 * stor7.length) + 192
        while idx < stor7.length:
            mem[t] = mem[s + 12 len 20]
            idx = idx + 1
            s = s + 32
            t = t + 32
            continue 
        return memory
          from (32 * stor7.length) + 128
           len (96 * stor7.length) + 64
    mem[128] = address(stor7.field_0)
    idx = 128
    s = 0
    while (32 * stor7.length) + 96 > idx:
        mem[idx + 32] = stor7[s].field_256
        idx = idx + 32
        s = s + 1
        continue 
    mem[(32 * stor7.length) + 128] = 32
    mem[(32 * stor7.length) + 160] = stor7.length
    idx = 0
    s = 128
    t = mem[64] + 64
    while idx < stor7.length:
        mem[t] = mem[s + 12 len 20]
        idx = idx + 1
        s = s + 32
        t = t + 32
        continue 
    return memory
      from mem[64]
       len (64 * stor7.length) + -mem[64] + 192
}

